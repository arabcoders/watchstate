        "@xterm/xterm": "^6.0.0",
        "cronstrue": "^3.9.0",
        "floating-vue": "^5.2.2",
        "hls.js": "^1.6.15",
        "marked": "^17.0.1",
        "marked-alert": "^2.1.2",
        "marked-base-url": "^1.1.8",
        "marked-gfm-heading-id": "^4.1.3",
        "moment": "^2.30.1",
        "nuxt": "^4.3.0",
        "pinia": "^3.0.4",
        "plyr": "^3.8.4",
