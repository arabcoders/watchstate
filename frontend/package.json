{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "format": "oxfmt --write app",
    "format:check": "oxfmt --check  app",
    "lint": "eslint .",
    "lint:ci": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "lint:tsc": "vue-tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:ci": "bun test --dots",
    "prepare:test": "nuxt prepare --envName=development"
  },
  "web-types": "./web-types.json",
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/core": "^14.2.1",
    "@vueuse/nuxt": "^14.2.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "cronstrue": "^3.12.0",
    "floating-vue": "^5.2.2",
    "hls.js": "^1.6.15",
    "marked": "^17.0.2",
    "marked-alert": "^2.1.2",
    "marked-base-url": "^1.1.8",
    "marked-gfm-heading-id": "^4.1.3",
    "moment": "^2.30.1",
    "nuxt": "^4.3.1",
    "pinia": "^3.0.4",
    "plyr": "^3.8.4",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2",
    "vue-toastification": "^2.0.0-rc.5",
    "vuedraggable": "^4.1.0",
    "@nuxt/eslint": "^1.15.1",
    "@nuxt/eslint-config": "^1.15.1"
  },
  "trustedDependencies": [
    "esbuild",
    "@parcel/watcher"
  ],
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/node": "25.2.3",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "oxfmt": "^0.33.0",
    "typescript": "^5.9.3",
    "vue-eslint-parser": "^10.4.0",
    "vue-tsc": "^3.2.4"
  },
  "eslintIgnore": [
    ".gitignore"
  ]
}
